{
    "name": "lde/laravel-pubsub-queue",
    "description": "Queue driver for Google Cloud Pub/Sub.",
    "keywords": [
        "lde",
        "laravel",
        "queue",
        "gcp",
        "google",
        "pubsub"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Munir Khakhi",
            "email": "munir131@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "google/cloud-pubsub": "^1.1",
        "illuminate/queue": "9.*",
        "laravel/helpers": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.10"
    },
    "autoload": {
        "psr-4": {
            "PubSub\\PubSubQueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PubSub\\PubSubQueue\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "PubSub\\PubSubQueue\\PubSubQueueServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
